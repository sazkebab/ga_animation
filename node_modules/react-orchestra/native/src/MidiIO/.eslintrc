{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/prefer-default-export": "off",
        "no-bitwise": "off",
        "react/prop-types": "off",
        "react/prefer-stateless-function": "off",
        "jsx-a11y/no-static-element-interactions": off
    }
}
